{
  "name": "@claw/analytics",
  "version": "1.0.0",
  "description": "BigQuery data warehouse and analytics infrastructure for all Claw apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node src/index.ts",
    "setup": "./setup-bigquery.sh",
    "setup:dry-run": "./setup-bigquery.sh --dry-run",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "bigquery",
    "analytics",
    "data-warehouse",
    "firebase",
    "gcp",
    "metrics"
  ],
  "author": "Claw Analytics Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^7.5.2",
    "firebase-admin": "^12.1.0",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
